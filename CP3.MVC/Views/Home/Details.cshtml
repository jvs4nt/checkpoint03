@model CP3.MVC.Application.Dtos.BarcoDto

@{
ViewData["Title"] = "Detalhes do Barco";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>@Model.Nome</h4>
    <p><strong>Modelo:</strong> @Model.Modelo</p>
    <p><strong>Ano:</strong> @Model.Ano</p>
    <p><strong>Tamanho:</strong> @Model.Tamanho</p>
</div>

<p>
    <a href="@Url.Action("Index", "Barco")" class="btn btn-secondary">Voltar</a>
</p>
